generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  username  String  @unique
  name      String
  bio       String?
  image     String?
  onboarded Boolean @default(false)
}
